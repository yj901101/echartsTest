            $("#vcode").click(function () {
                $("#imgvcode").attr("src", "../../Common/ValidateCode.ashx?s=" + Math.random());
            });
        });
        function Login() {
            if ($("#uname").val()== '' || $("#upwd").val() == '' || $("#txtvcode").val() == '') {
                alert('用户名或密码或验证码不能为空');
                return;
            }
            var data = $("#f1").serialize();
            $.post("../Login/Login", data, function (jsobj) {
                switch (jsobj.statu) {
                    case "falsevd": alert(jsobj.msg);
                        $("#imgvcode").attr("src", "../../Common/ValidateCode.ashx?s=" + Math.random());
                        $("#txtvcode").val('');
                        break;
                    case "falseuser": alert(jsobj.msg);
                        break;
                    case "falsepwd": alert(jsobj.msg);
                        break;
                    case "ok":
                        // alert(jsobj.msg);
                        window.location.href = ("../../" + jsobj.backurl + "");
                        break;
                }
            }, "json");
        };
