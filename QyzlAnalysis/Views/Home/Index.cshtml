@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zh-CN">
<head>
<title>区域经济知识产权分析系统-首页</title>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0;">
<link href="../../style/base.css" rel="stylesheet" type="text/css" />
<link href="../../style/index.css" rel="stylesheet" type="text/css" />
<script src="../../Scripts/jquery-1.9.0.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        $("#quit").click(Quit);
        $("#zl").parent().find("ul").find("li").each(function(){
           $(this).slideToggle();
          });
        $("#ecnomic").click(function(){
          $(this).parent().find("ul").find("li").each(function(){
           $(this).slideToggle();
          });
         });
        $("#zl").click(function(){
          $(this).parent().find("ul").find("li").each(function(){
           $(this).slideToggle();
          });
         });
    });
    function QyAddShow(a){
        var href=$(a).attr("href");
        $("#ifshow").remove();
        //$("#ifshow").attr("src",href);
    }
    function Quit() {
        $.getJSON("/Home/Quit?uname=@ViewData["UName"]",null, function (jsobj) {
                   switch (jsobj.statu) {
                   case "ok":
                            alert(jsobj.msg);
                            window.location.href=("../../" + jsobj.backurl + "");
                            break;
                   case "fail":
                              alert(jsobj.msg);
                              break;
                   }
         });
    }
</script>
<style type="text/css">
